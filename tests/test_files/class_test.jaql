class Foo {
  init(test) {
    this.test = test;
    print this;
  }
  hello (additional){
    print this.test+additional;
  }
}

var foo = Foo("hello world");
foo.hello("!");
print foo.init("new init");
foo.hello("!!");

class Circle {
  init(radius) {
    this.radius = radius;
  }

  area (){
    return this.radius * this.radius * 3; // 3.14159
  }
}

var circle = Circle(4);
print circle.area();
